# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
# Commented out route generated by play
# GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)


# Generated routes
GET   /reverse_geo_code                  com.vangav.vos_geo_server.controllers.reverse_geo_code.ControllerReverseGeoCode.getReverseGeoCode()
GET   /top_continents                   com.vangav.vos_geo_server.controllers.top_continents.ControllerTopContinents.getTopContinents()
GET   /top_countries                    com.vangav.vos_geo_server.controllers.top_countries.ControllerTopCountries.getTopCountries()
